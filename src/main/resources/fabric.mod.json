{
	"schemaVersion": 1,
	"id": "tapacraft",
	"version": "${version}",
	"name": "TapaCraft",
	"description": "Introducing spanish gastronomy into Minecraft!",
	"authors": [
		"JhonyRetro"
	],
	"contact": {
		"homepage": "${mod_homepage}",
		"sources": "${mod_github}",
		"issues": "${mod_github}/issues"
	},
	"license": "CC0-1.0",
	"icon": "assets/tapacraft/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"net.jhonyretro.tapacraft.TapaCraft"
		],
		"fabric-datagen": [
			"net.jhonyretro.tapacraft.TapaCraftDataGenerator"
		]
	},
	"mixins": [
		"tapacraft.mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.16.10",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}